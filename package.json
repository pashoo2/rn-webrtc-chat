{
  "name": "rnwebrtcchat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run kill & node node_modules/react-native/local-cli/cli.js start",
    "kill": "fuser -n tcp -k 8081",
    "start:server": "node ./signal-server",
    "debug:server": "node --inspect ./signal-server",
    "lint": "eslint --fix *.js",
    "lint:watch": "npx eslint-watch -w *.js",
    "test": "jest",
    "android:lint": "npm run kill & npm run lint:watch & react-native run-android",
    "android": "npm run kill & react-native run-android",
    "emu:list": "cd ~/Android/Sdk/tools/bin && ./avdmanager list avd",
    "emu:run": "cd ~/Android/Sdk/tools && ./emulator -avd"
  },
  "dependencies": {
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "react": "16.8.3",
    "react-native": "0.59.8",
    "react-native-permissions": "^1.1.1",
    "react-native-webrtc": "^1.69.1",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.0",
    "react-test-renderer": "16.8.3"
  },
  "optionalDependencies": {
    "eslint-watch": "^5.1.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
